{
    "name": "arrahmouni/response-helper",
    "version": "1.0.3",
    "description": "Response helper/builder for Laravel apps (web, ajax, api, internal).",
    "license": "MIT",
    "type": "library",
    "keywords": ["laravel", "response", "helper", "ar-rahmouni"],
    "authors": [
        {
            "name": "AMMAR RAHMOUNI",
            "email": "ammarrahmouni8@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.2",
        "illuminate/support": "^11.0 || ^12.0"
    },
    "autoload": {
        "psr-4": {
            "ArRahmouni\\ResponseHelper\\": "src/"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "extra": {
        "laravel": {
            "providers": [
                "ArRahmouni\\ResponseHelper\\ResponseServiceProvider"
            ]
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}

